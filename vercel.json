{
    "version": 2,
    "builds": [
        {
            "src": "./dist/server.js",
            "use": "@now/node"
        },
        {
            "src": "package.json",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/server.js"
        }
    ]
}